    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^28.1.8",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.36.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "28.0.8",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": ">=16.6",
    "npm": ">=8"
  },
  "files": [
    "dist"
  ],
