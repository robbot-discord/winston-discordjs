{
  "author": "Robert Smieja",
  "bugs": "https://github.com/robbot-discord/winston-discordjs/issues",
  "dependencies": {
    "@types/ws": "^8.5.3",
    "discord.js": "^13.6.0",
    "logform": "^2.4.0",
    "utility-types": "^3.10.0",
    "winston": "^3.7.2",
    "winston-transport": "^4.5.0"
  },
  "description": "Winston Transport using Discord.js",
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.17.10",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.5.0",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.1",
    "jest": "^27.5.1",
    "lint-staged": "^12.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "27.1.4",
    "typescript": "^4.6.4"
  },
  "engines": {
    "node": ">=14",
    "npm": ">=7"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "typescript",
    "nodejs",
    "winston",
    "discord",
    "logging",
    "log",
    "transport"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "winston-discordjs",
  "private": false,
  "repository": "https://github.com/robbot-discord/winston-discordjs",
  "scripts": {
    "build": "npm-run-all --parallel build:js lint",
    "build:js": "tsc",
    "clean": "rimraf dist/",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint --fix ./src/**/*",
    "lint:staged": "lint-staged",
    "postversion": "git push && git push --tags",
    "prepack": "npm-run-all clean build",
    "prepublishOnly": "npm-run-all --parallel test lint",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "test": "jest",
    "version": "npm run lint:fix && git add -A src"
  },
  "types": "dist/index.d.ts",
  "version": "3.0.0"
}
