{
  "author": "Robert Smieja",
  "bugs": "https://github.com/robbot-discord/winston-discordjs/issues",
  "dependencies": {
    "@types/ws": "^7.2.6",
    "discord.js": "^12.3.1",
    "logform": "^2.2.0",
    "utility-types": "^3.10.0",
    "winston": "^3.3.3",
    "winston-transport": "^4.4.0"
  },
  "description": "Winston Transport using Discord.js",
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.1",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "jest": "^26.4.1",
    "lint-staged": "^10.2.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "ts-jest": "26.2.0",
    "typescript": "^4.0.2"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "typescript",
    "nodejs",
    "winston",
    "discord",
    "logging",
    "log",
    "transport"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "main": "dist/index.js",
  "name": "winston-discordjs",
  "private": false,
  "repository": "https://github.com/robbot-discord/winston-discordjs",
  "scripts": {
    "build": "npm-run-all --parallel build:js lint",
    "build:js": "tsc",
    "clean": "rimraf dist/",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint --fix ./src/**/*",
    "lint:staged": "lint-staged",
    "postversion": "git push && git push --tags",
    "prepack": "npm-run-all clean build",
    "prepublishOnly": "npm-run-all --parallel test lint",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "test": "jest",
    "version": "npm run lint:fix && git add -A src"
  },
  "types": "dist/index.d.ts",
  "version": "2.1.1"
}
