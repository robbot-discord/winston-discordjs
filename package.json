{
  "author": "Robert Smieja",
  "bugs": "https://github.com/robbot-discord/winston-discordjs/issues",
  "dependencies": {
    "@types/ws": "^7.2.2",
    "discord.js": "^12.0.1",
    "logform": "^2.1.2",
    "utility-types": "^3.10.0",
    "winston": "^3.2.1",
    "winston-transport": "^4.3.0"
  },
  "description": "Winston Transport using Discord.js",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "ts-jest": "25.2.1",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">=12",
    "npm": ">=6"
  },
  "files": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": [
        "lint-staged"
      ]
    }
  },
  "keywords": [
    "typescript",
    "nodejs",
    "winston",
    "discord",
    "logging",
    "log",
    "transport"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/*.{js,ts,jsx,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "winston-discordjs",
  "private": false,
  "repository": "https://github.com/robbot-discord/winston-discordjs",
  "scripts": {
    "build": "npm-run-all --parallel build:js lint",
    "build:js": "tsc",
    "clean": "rimraf dist/",
    "lint": "eslint ./src/**/*",
    "lint:fix": "eslint --fix ./src/**/*",
    "lint:staged": "lint-staged",
    "postversion": "git push && git push --tags",
    "prepack": "npm-run-all clean build",
    "prepublishOnly": "npm-run-all --parallel test lint",
    "prettier": "prettier --write '*.{js,ts,jsx,tsx,json,md}'",
    "test": "jest",
    "version": "npm run lint:fix && git add -A src"
  },
  "types": "dist/index.d.ts",
  "version": "1.1.0"
}
